{
    "manifest_version": 2,
    "name": "Random Quote Injector for Gmail",
    "version": "1.0",
    "description": "This extension injects a random quote from your google sheet into your Gmail drafts!",
    "icons": {
      "16": "icons/quotes16.png",
      "32": "icons/quotes32.png",
      "128": "icons/quotes128.png"
    },
    "permissions": [
      "https://mail.google.com/",
      "activeTab",
      "identity",
      "storage",
      "management"
    ],
    "options_page": "options.html",
    "browser_action": {
      "default_icon": "icons/refresh32.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
      }
    ],
    "oauth2": {
      "client_id": "51718967871-218k0vrh4h3rfci12is1mvv0i2env7ov.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/spreadsheets.readonly"]
    }
  }
  